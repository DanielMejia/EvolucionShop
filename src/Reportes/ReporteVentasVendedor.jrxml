	<subDataset name="ReporteVentasVendedor" uuid="681b2062-3583-4fb7-93f8-55dc39186dfb">
		<parameter name="idvendedor" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select * from 
(
select fecha, sum(montototal) as ventatotal
from "venta" natural join "usuario"
where identificacionusuario = $P{idvendedor}
group by fecha
) as a 
natural join
(
select identificacionusuario,idventa,fecha,descuento,montototal,nombreusuario,apellidosusuario, direccion,estado,telefonousuario
from "venta" natural join "usuario"
where identificacionusuario = $P{idvendedor}
) as b
order by fecha]]>
		</queryString>
		<field name="fecha" class="java.sql.Date"/>
		<field name="ventatotal" class="java.lang.Long"/>
		<field name="identificacionusuario" class="java.lang.String"/>
		<field name="idventa" class="java.lang.Integer"/>
		<field name="descuento" class="java.lang.Integer"/>
		<field name="montototal" class="java.lang.Integer"/>
		<field name="nombreusuario" class="java.lang.String"/>
		<field name="apellidosusuario" class="java.lang.String"/>
		<field name="direccion" class="java.lang.String"/>
		<field name="estado" class="java.lang.Boolean"/>
		<field name="telefonousuario" class="java.lang.String"/>
		<variable name="sumadescuento" class="java.lang.Integer" calculation="Sum">
			<variableExpression><![CDATA[$F{descuento}]]></variableExpression>
		</variable>
		<variable name="sumamonto" class="java.lang.Integer" calculation="Sum">
			<variableExpression><![CDATA[$F{montototal}]]></variableExpression>
		</variable>
	</subDataset>
